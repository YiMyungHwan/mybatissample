<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mx.mybatissample.dao.IUserDAO">

    <!-- 사용자 조회 -->
    <select id="listUser" resultType="com.mx.mybatissample.vo.UserEntity">
    select ID
         , user_login
         , user_pass
      from wp_users
    </select>
</mapper>